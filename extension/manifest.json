{
    "name": "Bruinwalk Extension",
    "description": "Unofficial Bruinwalk extension",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://be.my.ucla.edu/*",
        "https://sa.ucla.edu/*",
        "https://www.bruinwalk.com/*"
    ]
}